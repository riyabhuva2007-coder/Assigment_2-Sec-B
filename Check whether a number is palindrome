#include <stdio.h>
int main() {
    int num, rev = 0, temp;
    printf("Enter the number to be checked:");
    scanf("%d", &num);
    temp = num;
    while (num) {
        rev = rev * 10 + num % 10;
        num=num/10;
    }
    if (temp == rev)
        printf("Palindrome\n");
    else
        printf("Not a palindrome\n");
    return 0;
}
