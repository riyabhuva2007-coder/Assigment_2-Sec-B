#include <stdio.h>
int main() {
    int num, sum = 0, temp, digit;
    printf("Enter the number to be checked:");
    scanf("%d", &num);
    temp = num;
    while (temp) {
        digit = temp % 10;
        sum += digit * digit * digit;
        temp=temp/10;
    }
    if (sum == num)
        printf("Armstrong\n");
    else
        printf("Not Armstrong\n");
    return 0;
}
